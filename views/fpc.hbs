{{!-- popup form --}}
 <div class="modal fade" id="rm-modal-form" tabindex="-1" role="dialog" aria-labelledby="rm-modal-form" aria-hidden="true" data-backdrop="false">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
        <div class="modal-header">
            <h3 class="modal-title">ADD COMPONENTS <span id="title"></span></h3>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <div class="modal-body">
            <form id="form__">
                <div class="form-group row">
                    <input type="text" class="form-control invisible" id="fpc_guid" value="{{fpc_guid}}" style="position:absolute;" required readonly>
                    <div class="col-md-5 mb-3">
                        <label>RM</label>
                        <input type="text" class="form-control" name="fp_cp_rm_code" placeholder="RM" value="" required>
                    </div>
                    <div class="col-md-7 mb-3">
                        <label>RM Desc</label>
                        <input type="text" class="form-control" name="rm_desc" readonly>
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col-md-4 mb-3">
                        <label>Qty</label>
                        <input type="text" class="form-control" name="fp_cp_qty" placeholder="Qty" value="" required>
                    </div>
                    <div class="col-md-7 mb-3">
                        <label for="unit">Unit</label>
                        <select class="form-control selectpicker" id="sp_unit" name="fp_cp_unit" data-live-search="true">
                            <option value="-">-</option>
                            {{#each __data_pop_unit}}
                                <option value="{{pop_guid}}">{{pop_desc}}</option>
                            {{/each}}
                        </select>
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col-md-7 mb-3">
                        <label>Price</label>
                        <input type="text" class="form-control" name="fp_cp_price" placeholder="Price" value="0" required>
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-sm pull-right">
                        <button type="submit" class="btn btn-outline btn-primary" id="btn_save_pop"><i class="fa fa-save"></i> Save</button>
                    </div>
                    <div class="col-sm pull-right">
                        <a href="#"  type="button" class="btn btn-outline btn-primary" id="btn_close" data-dismiss="modal">
                            <i class="fa fa-times"></i> Cancel
                        </a>
                    </div>
                </div>
                <div class="form-group row invisible" style="position: absolute;">
                    <div class="col-md-4 mb-3">
                        <label>Updating by</label>
                        <input type="text" class="form-control" name="updated_by" readonly>
                    </div>
                    <div class="col-md-4 mb-3">
                        <label>Id</label>
                        <input type="text" class="form-control" id="fp_fpc_guid" name="{{fp_fpc_guid}}" readonly>
                        <input type="text" class="form-control" id="fp_guid" name="fp_guid" value="{{fp_guid}}" readonly>
                        <input type="text" class="form-control" id="act" name="act" value="{{act}}" readonly>
                        <input type="text" class="form-control" id="fp_cp_type" name="fp_cp_type" readonly>
                    </div>
                </div>
                <hr/>
            </form>
            
        </div>
        </div>
    </div>
</div>
