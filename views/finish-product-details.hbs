<!DOCTYPE html>
<html lang="en">

{{> head}}
{{> css}}

<style>
    .col-md-6{
        width: 0%;
    }
    #dtTbl_pop{
        width: 100% !important;
    }
</style>
<body>

    <div id="wrapper">

        {{> navbar}}

        {{> sidenav}}

        <!-- Page Content -->
        <div id="page-wrapper">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">
                        <h3 class="page-header">FINISH PRODUCT UPDATES</h3>
                    </div>
                    <!-- /.col-lg-12 -->
                </div>
                <div class="row">&nbsp;</div>
                <div class="row">
                    <form id="form_">
                        <div class="col-sm pull-left" >
                            <a href="#"  type="button" class="btn btn-outline btn-primary" id="btn_fpc">
                                Components <i class="fa fa-long-arrow-right"></i>
                            </a>
                        </div>
                        <div class="col-sm pull-right">&nbsp;</div>
                        <div class="col-sm pull-right cancel">
                            <a href="/finish-product"  type="button" class="btn btn-outline btn-primary" id="btn_cancel">
                                <i class="fa fa-times"></i> Cancel
                            </a>
                        </div>
                        <div class="col-sm pull-right">&nbsp;</div>
                        <div class="col-sm pull-right">
                            <button type="submit" class="btn btn-outline btn-primary" id="btn_save"><i class="fa fa-save"></i> Save</button>
                        </div>
                        <div class="row">&nbsp;</div>
                        <div class="row">&nbsp;</div>
                        <div class="form-group row">
                            <div class="col-md-9 mb-3">
                                <label for="product_family_desc">Finish Product Desc</label>
                                <input type="text" class="form-control" name="fp_desc" placeholder="Finish Product Desc" value="" required>
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-md-4 mb-3">
                                <label for="product_family">Product Family</label>
                                <select class="form-control selectpicker" id="sp_product_family" name="fp_prod_family" data-live-search="true">
                                    <option value="-">-</option>
                                    {{#each __data_pop_product_family}}
                                        <option value="{{pop_guid}}">{{pop_desc}}</option>
                                    {{/each}}
                                </select>
                            </div>
                            <div class="col-md-4 mb-3">
                                <label for="sp_origin">Origin</label>
                                <select class="form-control selectpicker" id="sp_origin" name="fp_origin" data-live-search="true">
                                    <option value="-">-</option>
                                    {{#each __data_pop_origin}}
                                        <option value="{{pop_guid}}">{{pop_desc}}</option>
                                    {{/each}}
                                </select>
                            </div>
                            {{!-- <div class="col-md-4 mb-3">
                                <label for="h">H</label>
                                <input type="text" class="form-control" name="h" placeholder="H" value="" required>
                            </div> --}}
                        </div>
                        {{!-- <div class="form-group row">
                            <div class="col-md-4 mb-3">
                                <label for="created_date">Created Date</label>
                                <input type="text" class="form-control" name="created_date" id="created_date" placeholder="Created Date" autocomplete="off" required>
                            </div>
                            <div class="col-md-4 mb-3">
                                <label for="receipt_last_updated">Receipt Last Updated</label>
                                <input type="text" class="form-control" id="receipt_last_updated" name="receipt_last_updated" placeholder="Receipt Last Updated" autocomplete="off" required >
                            </div>
                        </div> --}}
                        <div class="form-group row">
                            <div class="col-md-4 mb-3" style="text-align: center;">
                                <label for="box_size_w">Box Size</label>
                                <div class="form-group row">
                                    <div class="col-md-4 mb-3">
                                        <label for="box_size_l" class="col-sm-1 col-form-label col-form-label-sm" style="padding-left: 0px;">L</label>
                                        <div class="col-md-2">
                                            <input type="text" class="form-control numeric" name="fp_box_size_l" placeholder="L" style="width: 71px;" required autocomplete="off">
                                        </div>
                                    </div>
                                    <div class="col-md-4 mb-3">
                                        <label for="box_size_w" class="col-sm-1 col-form-label col-form-label-sm" style="padding-left: 0px;">W</label>
                                        <div class="col-md-2">
                                            <input type="text" class="form-control numeric" name="fp_box_size_w" placeholder="W" style="width: 71px;" required autocomplete="off">
                                        </div>    
                                    </div>
                                    <div class="col-md-4 mb-3">
                                        <label for="box_size_h" class="col-sm-1 col-form-label col-form-label-sm" style="padding-left: 0px;">H</label>
                                        <div class="col-md-2">
                                            <input type="text" class="form-control numeric" name="fp_box_size_h" placeholder="H" style="width: 71px;" required autocomplete="off">
                                        </div>
                                    </div>
                                </div>
                            </div>
                             {{!-- <div class="col-md-4 mb-3">
                                <label for="sp_creator">Creator</label>
                                <select class="form-control selectpicker" id="sp_creator" name="creator" data-live-search="true">
                                    <option value="-">-</option>
                                    {{#each __data_pop_creator}}
                                        <option value="{{id}}">{{pop_desc}}</option>
                                    {{/each}}
                                </select>
                            </div> --}}
                        </div>
                        <div class="form-group row">
                            <div class="col-md-4 mb-3 form-check form-check-inline">
                                <div class="row">&nbsp;</div>
                                <input class="form-check-input" type="checkbox" value="" id="ck_validated" name="fp_validated">
                                <label class="form-check-label" for="ck_validated">
                                    Validated
                                </label>
                                <input class="form-check-input" type="checkbox" value="" id="ck_active" name="fp_active">
                                <label class="form-check-label" for="ck_active">
                                    Active
                                </label>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="form-group row">&nbsp;</div>
                            <div class="form-group row">
                                {{!-- <div class="col-md-4 mb-3"> --}}
                                    <strong>SALE CALCULATION</strong><hr>
                                {{!-- </div> --}}
                            </div>
                            <div class="form-group row">
                                <div class="col-sm-3 mb-3">
                                    <label for="labour">Extra Cost</label>
                                    <input type="text" class="form-control number-seperator numeric" id ="extra_cost" name="fp_sc_extra_cost" placeholder="Extra Cost" autocomplete="off" value="0" required>
                                </div>
                                <div class="col-sm-3 mb-3">
                                    <label for="labour">Labour</label>
                                    <input type="text" class="form-control number-seperator numeric" id ="labour" name="fp_sc_extra_labour" placeholder="Labour" autocomplete="off" value="0" required>
                                </div>
                                <div class="col-sm-3 mb-3">
                                    <label for="cost">Cost</label>
                                    <input type="text" class="numeric form-control number-seperator" id ="cost" name="fp_sc_cost" placeholder="Cost" autocomplete="off" value="0" required>
                                </div>
                                <div class="col-sm-3 mb-3">
                                    <label for="total_cost">Total Cost</label>
                                    <input type="text" class="numeric form-control number-seperator" id ="total_cost" placeholder="Total Cost" autocomplete="off" value="0" readonly>
                                </div>
                                
                            </div>
                            <div class="row" style="text-align: center;">
                                <div class="col-lg">
                                    <label>WHOLESALE</label>
                                </div>
                            </div>
                            <div class="form-group row">
                                <div class="col-sm-4 mb-3">
                                    <label for="ws_sale">Sale</label>
                                    <input type="text" class="numeric form-control number-seperator" id ="ws_sale" name="fp_sc_wholesale_sale" placeholder="Sale" autocomplete="off" value="0" required>
                                </div>
                                <div class="col-sm-4 mb-3">
                                    <label for="ws_profit">Profit</label>
                                    <input type="text" class="numeric form-control number-seperator" id ="ws_profit" name="fp_sc_wholesale_profit" placeholder="Profit" autocomplete="off" value="0" required readonly>
                                </div>
                                <div class="col-sm-4 mb-3">
                                    <label for="ws_markup">Markup</label>
                                    <input type="text" class="form-control" id ="ws_markup" placeholder="Markup" autocomplete="off" value="0" required readonly>
                                </div>
                            </div>
                            <div class="row" style="text-align: center;">
                                <div class="col-lg">
                                    <label>BUSINESS</label>
                                </div>
                            </div>
                            <div class="form-group row">
                                <div class="col-sm-4 mb-3">
                                    <label for="b_sale">Sale</label>
                                    <input type="text" class="numeric form-control number-seperator" id ="b_sale" name="fp_sc_business_sale" placeholder="Sale" autocomplete="off" value="0" required>
                                </div>
                                <div class="col-sm-4 mb-3">
                                    <label for="b_profit">Profit</label>
                                    <input type="text" class="numeric form-control number-seperator" id ="b_profit" name="fp_sc_business_profit" placeholder="Profit" autocomplete="off" value="0" required readonly>
                                </div>
                                <div class="col-sm-4 mb-3">
                                    <label for="b_markup">Markup</label>
                                    <input type="text" class="form-control" id ="b_markup" placeholder="Markup" autocomplete="off" value="0" required readonly>
                                </div>
                            </div>
                            <div class="row" style="text-align: center;">
                                <div class="col-lg">
                                    <label>RETAIL</label>
                                </div>
                            </div>
                            <div class="form-group row">
                                <div class="col-sm-4 mb-3">
                                    <label for="r_sale">Sale</label>
                                    <input type="text" class="numeric form-control number-seperator" id ="r_sale" name="fp_sc_retail_sale" placeholder="Sale" autocomplete="off" value="0" required>
                                </div>
                                <div class="col-sm-4 mb-3">
                                    <label for="r_profit">Profit</label>
                                    <input type="text" class="numeric form-control number-seperator" id ="r_profit" name="fp_sc_retail_profit" placeholder="Profit" autocomplete="off" value="0" required readonly>
                                </div>
                                <div class="col-sm-4 mb-3">
                                    <label for="r_markup">Markup</label>
                                    <input type="text" class="form-control" id ="r_markup" placeholder="Markup" autocomplete="off" value="0" required readonly>
                                </div>
                            </div>
                            
                        </div>
                        {{!-- <div class="form-group">
                            <div class="form-check">
                                <input class="form-check-input" type="checkbox" value="" id="ck_active" name="active">
                                <label class="form-check-label" for="ck_active">
                                    Active
                                </label>
                            </div>
                        </div> --}}
                        <div class="form-group row invisible">
                            <div class="col-md-4 mb-3">
                                <label>Created by</label>
                                <input type="text" class="form-control" name="fp_created_by" readonly>
                            </div>
                            <div class="col-md-4 mb-3 invisible">
                                <label>Id</label>
                                <input type="text" class="form-control" id="fp_guid" name="fp_guid" value="{{fpid}}" readonly>
                                <input type="text" class="form-control" id="act" name="act" value="{{act}}" readonly>
                            </div>
                        </div>
                    </form>
                </div>
                <!-- /.row -->
            </div>
            <!-- /.container-fluid -->
        </div>
        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->
        {{!-- popup modal form --}}
        <!-- Modal -->
        
       {{> pop}}
        {{!-- spinner --}}
        <div id="spinner-modal" class="modal fade spinner-bg modal-popup" tabindex="-1" role="dialog" data-backdrop="false">
            <div role="document">
                <span class="fa fa-spinner fa-pulse fa-3x fa-fw loader-custom"></span>
                {{!-- <div class="row">
                    <div class="col-md-3">
                        <div class="loader" id="loader-4">
                            <span></span>
                            <span></span>
                            <span></span>
                        </div>
                    </div>
               </div> --}}
            </div>
        </div>
    </div>
    
    <!-- /#wrapper -->
{{> scripts}}
</body>

</html>
